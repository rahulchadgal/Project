package linkedList;

public class KnthNodeRemove {
	static class Node {
		public int data;
		public Node next;

		// Constructor for Node with data and next node
		public Node(int data1, Node next1) {
			data = data1;
			next = next1;
		}

		// Constructor for Node with only data (next set to null)
		public Node(int data1) {
			data = data1;
			next = null;
		}
	}

	public static void main(String[] args) {
		int[] arr = { 1, 2, 3, 4, 5 };
		int N = 3;
		Node head = new Node(arr[0]);
		head.next = new Node(arr[1]);
		head.next.next = new Node(arr[2]);
		head.next.next.next = new Node(arr[3]);
		head.next.next.next.next = new Node(arr[4]);

		// Delete the Nth node from the end and print the modified linked list
		head = DeleteNthNodefromEnd(head, N);

	}

	private static Node DeleteNthNodefromEnd(Node head, int n) {
		// TODO Auto-generated method stub
		return null;
	}

}
