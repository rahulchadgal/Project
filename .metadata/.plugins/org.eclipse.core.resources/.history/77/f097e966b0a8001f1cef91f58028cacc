package service;

import java.util.ArrayList;
import java.util.List;

import models.Floors;

public class ParkingLot {

	private static ParkingLot instance;
	List<Floors> floors = new ArrayList<>();

	public ParkingLot(List<Floors> floor) {
		this.floors = floors;
	}

	public static ParkingLot getInstance(List<Floors> floors) {
		if (instance == null)
			return new ParkingLot(floors);
		return instance;
	}

}
